{
  "name": "js-tools",
  "version": "1.0.0",
  "description": "Describe js-tools here",
  "private": true,
  "scripts": {
    "build": "nwb build-react-app",
    "clean": "nwb clean-app",
    "start": "nwb serve-react-app",
    "test": "eslint src/**/*.js && nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "prettier":
      "prettier --write '*.{js,prettierrc,json}' '{src,tests}/**/*.js'",
    "precommit": "npm run prettier && npm test",
    "prepush": "npm test"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "eslint": "4.19.0",
    "eslint-plugin-react": "7.7.0",
    "husky": "0.14.3",
    "nwb": "0.21.x",
    "nwb-sass": "0.8.3",
    "prettier": "1.11.1"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
